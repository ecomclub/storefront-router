{
  "name": "@ecomplus/storefront-router",
  "version": "0.3.0",
  "description": "Universal JS router for E-Com Plus storefront",
  "main": "dist/router.min.js",
  "module": "src/index.js",
  "browser": "dist/router.min.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "serve": "webpack-dev-server",
    "doc": "rm -rf ./docs/*; jsdoc ./src -r ./README.md -d ./docs -t ./node_modules/docdash",
    "release": "rm -rf ./dist; npm run build & standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomclub/storefront-router.git"
  },
  "keywords": [
    "ecomplus",
    "storefront",
    "router",
    "ecommerce",
    "universal-app"
  ],
  "author": "E-Com Club <ti@e-com.club>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomclub/storefront-router/issues"
  },
  "homepage": "https://github.com/ecomclub/storefront-router#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@ecomplus/client": "^0.8.0",
    "@ecomplus/utils": "^0.6.0",
    "core-js": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-loader": "^8.0.6",
    "docdash": "^1.1.1",
    "husky": "^3.0.1",
    "jsdoc": "^3.6.3",
    "standard": "^13.1.0",
    "standard-version": "^6.0.1",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "last 1 version",
    "not dead",
    "> 0.2%"
  ]
}
